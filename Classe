# APS


public class consumo_luz {

    double PotenciaProduto;
    double TempoDeUso;
    double PrecoFinal;
    double Produto_kWh;
    double Preco_kWh = 0.649;

    private double luz() {

            Produto_kWh = PotenciaProduto * TempoDeUso ;

            PrecoFinal = Produto_kWh * Preco_kWh ;

        return PrecoFinal;
    }
}

